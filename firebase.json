{
    "functions": {
        "source": "functions"
    },
    "firestore": {
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    },
    "storage": {
        "rules": "storage.rules"
    },
    "hosting": {
        "public": "public",
        "predeploy": ["npm run build", "npm run firebase:seed"],
        "rewrites": [
            {
                "source": "**",
                "function": "sveltekitSSR",
                "region": "asia-southeast1",
                "pinTag": true
            }
        ]
    },
    "remoteconfig": {
        "template": "remoteconfig.template.json"
    },
    "emulators": {
        "auth": {
            "port": 9099
        },
        "functions": {
            "port": 5001
        },
        "firestore": {
            "port": 8080
        },
        "hosting": {
            "port": 8000
        },
        "pubsub": {
            "port": 8085
        },
        "storage": {
            "port": 9199
        },
        "ui": {
            "enabled": true
        },
        "singleProjectMode": true
    },
    "extensions": {
        "firestore-send-email": "firebase/firestore-send-email@0.1.27",
        "storage-resize-images": "firebase/storage-resize-images@0.1.39"
    }
}
