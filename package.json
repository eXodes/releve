{
    "name": "releve",
    "version": "0.0.0",
    "type": "module",
    "exports": {
        ".": "./packages"
    },
    "scripts": {
        "prepare": "svelte-kit sync",
        "preinstall": "cd functions && npm install",
        "dev": "vite dev",
        "build": "vite build",
        "bundle": "npm run --prefix functions build && npm run build",
        "preview": "vite preview",
        "test": "playwright test",
        "precheck": "npm run --prefix functions check",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check --plugin-search-dir=. . && eslint .",
        "format": "prettier --write --plugin-search-dir=. .",
        "firebase:emu:start": "firebase emulators:start",
        "firebase:seed": "tsx ./seeder",
        "firebase:seed:faker": "tsx ./seeder/faker",
        "firebase:deploy": "firebase deploy --only hosting,"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.0.0",
        "@playwright/test": "^1.33.0",
        "@sveltejs/adapter-auto": "^2.0.1",
        "@sveltejs/kit": "^1.16.3",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/line-clamp": "^0.4.4",
        "@tailwindcss/typography": "^0.5.9",
        "@types/cookie": "^0.5.1",
        "@types/lodash-es": "^4.17.7",
        "@types/md5": "^2.3.2",
        "@types/nodemailer": "^6.4.7",
        "@types/validator": "^13.7.17",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "postcss": "^8.4.23",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.0",
        "prettier-plugin-tailwindcss": "^0.2.8",
        "svelte": "^3.59.1",
        "svelte-adapter-firebase": "^0.15.0",
        "svelte-check": "^3.3.2",
        "svelte-match-media": "^1.2.1",
        "svelte-preprocess": "^5.0.3",
        "tailwindcss": "^3.3.2",
        "tslib": "^2.5.0",
        "tsx": "^3.12.7",
        "typescript": "^5.0.4",
        "vite": "^4.3.5"
    },
    "dependencies": {
        "@rgossiaux/svelte-headlessui": "^1.0.2",
        "cookie": "^0.5.0",
        "date-fns": "^2.30.0",
        "dotenv": "^16.0.3",
        "firebase": "^9.22.0",
        "firebase-admin": "^11.8.0",
        "joi": "^17.9.2",
        "lodash-es": "^4.17.21",
        "md5": "^2.3.0",
        "qs": "^6.11.1",
        "svelte-hero-icons": "^5.0.0",
        "validator": "^13.9.0",
        "vest": "^4.6.11"
    },
    "node": "18"
}
