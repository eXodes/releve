{
    "name": "releve",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "sync": "svelte-kit sync",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "test:ui": "playwright test --ui",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check . && eslint .",
        "lint:fix": "prettier --write . && eslint .",
        "format": "prettier --write .",
        "firebase:emu:start": "firebase emulators:start",
        "firebase:emu:frontend": "firebase emulators:start --only=hosting",
        "firebase:emu:backend": "firebase emulators:start --only=auth,firestore,storage,extensions",
        "firebase:seed": "tsx --tsconfig ./tsconfig.json ./seeder",
        "firebase:seed:e2e": "tsx --tsconfig ./tsconfig.json ./seeder/e2e",
        "firebase:seed:faker": "tsx --tsconfig ./tsconfig.json ./seeder/faker",
        "firebase:deploy": "firebase deploy --only functions,hosting,",
        "firebase:deploy:functions": "firebase deploy --only functions"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.3.1",
        "@playwright/test": "^1.40.1",
        "@sveltejs/adapter-auto": "^3.0.1",
        "@sveltejs/vite-plugin-svelte": "^3.0.1",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@types/cookie": "^0.6.0",
        "@types/lodash-es": "^4.17.12",
        "@types/md5": "^2.3.5",
        "@types/nodemailer": "^6.4.14",
        "@types/nprogress": "^0.2.3",
        "@types/validator": "^13.11.7",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "@vite-pwa/sveltekit": "^0.3.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "is-ci": "^3.0.1",
        "postcss": "^8.4.32",
        "prettier": "^3.1.1",
        "prettier-plugin-svelte": "^3.1.2",
        "prettier-plugin-tailwindcss": "^0.5.9",
        "svelte-adapter-firebase": "^0.15.0",
        "svelte-check": "^3.6.2",
        "svelte-match-media": "^1.2.1",
        "svelte-popperjs": "^1.3.2",
        "svelte-preprocess": "^5.1.3",
        "tailwindcss": "^3.4.0",
        "tslib": "^2.6.2",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3",
        "vite-plugin-pwa": "^0.17.4"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.8",
        "@sentry/profiling-node": "^1.3.2",
        "@sentry/sveltekit": "^7.90.0",
        "@sveltejs/kit": "^2.0.6",
        "cookie": "^0.6.0",
        "date-fns": "^3.0.6",
        "dotenv": "^16.3.1",
        "firebase": "^10.7.1",
        "firebase-admin": "^12.0.0",
        "joi": "^17.11.0",
        "lodash-es": "^4.17.21",
        "md5": "^2.3.0",
        "nprogress": "^0.2.0",
        "qs": "^6.11.2",
        "svelte": "^4.2.8",
        "svelte-headlessui": "^0.0.32",
        "svelte-hero-icons": "^5.0.0",
        "svelte-transition-classes": "^0.1.1",
        "validator": "^13.11.0",
        "vest": "^5.2.3",
        "vite": "^5.0.10"
    },
    "node": "18"
}
